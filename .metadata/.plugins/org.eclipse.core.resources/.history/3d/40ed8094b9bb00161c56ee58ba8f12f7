package day6;

public class D6Q2 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int[][] grid = new int[8][26];
		// Character c = Character.forDigit(11, 36);
		// System.out.println(c);
		// int d = Character.getNumericValue(c);
		// System.out.println(d + "");

		for (String arg : args) {
			char[] chars = arg.toCharArray();
			for (int ix = 0; ix < chars.length; ix++) {
				int cix = Character.getNumericValue(chars[ix])-10;
				int temp = grid[ix][cix];
				System.out.println("Temp: " + temp + " ix=" + ix + " cix=" + cix);
				temp++;
				grid[ix][cix] = temp;
			}
		}
		
		for(int i = 0; i < 8; i++){
			int max = 0;
			int maxCol = 0;
			for(int j = 0; j < 26; j++){
				if(grid[i][j]>max){
					max = grid[i][j];
					maxCol=j;
				}
			}
			Character res = Character.forDigit(maxCol+10, 36);
			System.out.println("MaxCol=" + res + " value="+max);
		}

	}

}
